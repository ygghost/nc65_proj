<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<!-- 环境变量 -->
	<bean id="context" class="nc.vo.uif2.LoginContext" />

	<!-- 对象转换器工厂，由此获取操作对象的特征信息 -->
	<bean id="boadatorfactory"
		class="nc.ui.pubapp.uif2app.view.value.AggVOMetaBDObjectAdapterFactory" />
	<!-- 适配分页start -->
	<bean id="pageBar" class="nc.ui.uif2.components.pagination.PaginationBar" />
	<bean id="pageDelegator"
		class="nc.ui.pubapp.uif2app.actions.pagination.BillModelPaginationDelegator">
		<constructor-arg ref="ManageAppModel" />
		<property name="paginationQuery" ref="pageQuery" />
	</bean>
	<bean id="pageQuery" class="nc.ui.so.m30.billui.model.SaleOrderPageService"></bean>
	<bean id="pageMediator" class="nc.ui.scmpub.page.model.SCMBillPageMediator"
		init-method="init">
		<property name="listView" ref="listView" />
		<property name="recordInPage" value="10" />
		<property name="cachePages" value="10" />
		<property name="pageDelegator" ref="pageDelegator" />
	</bean>
	<!-- 适配分页end -->
	<!-- 管理应用模型 -->
	<bean id="ManageAppModel" class="nc.ui.pubapp.uif2app.model.BillManageModel">
		<property name="businessObjectAdapterFactory" ref="boadatorfactory"></property>
		<property name="context" ref="context"></property>
	</bean>
	
	

	<!-- 数据模型管理器，主要负责各种方式的模型初始化 -->
	<bean id="modelDataManager" class="nc.ui.scmpub.page.model.SCMBillPageModelDataManager">
		<property name="model" ref="ManageAppModel"></property>
		<property name="pageQuery" ref="pageQuery"></property>
		<property name="pageDelegator" ref="pageDelegator"></property>
		<property name="pagePanel" ref="listToolbarPnl" />
	</bean>
	<bean id="trantypeTempMender"
		class="nc.ui.pubapp.uif2app.funcnode.trantype.TrantypeBillTemplateMender">
		<constructor-arg ref="context" />
	</bean>

	<!-- 模板容器，负责对模板的统一装载 -->
	<bean id="templateContainer" class="nc.ui.pubapp.uif2app.view.TemplateContainer"
		init-method="load">
		<property name="context" ref="context" />
		<property name="billTemplateMender" ref="trantypeTempMender"></property>
	</bean>

	<!-- 界面元素 ########################################################### -->
	<!-- 列表视图 -->
	<bean id="listView" class="nc.ui.so.m30.billui.view.SaleOrderBillListView"
		init-method="initUI">
		<property name="model" ref="ManageAppModel"></property>
		<property name="multiSelectionMode" value="0" />
		<property name="templateContainer" ref="templateContainer"></property>
		<property name="paginationBar" ref="pageBar"/>	
		<!-- 列表自定义项处理 -->
		<property name="userdefitemListPreparator">
			<bean name="userdefAndMarAsstListPreparator"
				class="nc.ui.pubapp.uif2app.view.CompositeBillListDataPrepare">
				<property name="billListDataPrepares">
					<list>
						<ref bean="userdefitemlistPreparator" />
						<ref bean="marAsstPreparator" />
					</list>
				</property>
			</bean>
		</property>
	</bean>
	<!-- 源头单据号超链接 -->
	<bean id="linkQueryHyperlinkMediator"
		class="nc.ui.pubapp.uif2app.linkquery.LinkQueryHyperlinkMediator">
		<property name="model" ref="ManageAppModel" />
		<property name="srcBillIdField" value="cfirstid" />
		<!-- 超链字段名 -->
		<property name="srcBillNOField" value="vfirstcode" />
		<!-- 保存源单据类型的字段名 -->
		<property name="srcBillTypeField" value="vfirsttype" />
		<!-- 保存源单据类型的字段所在位置，0：表头；1：表体 -->
		<property name="srcBillTypeFieldPos" value="1" />
	</bean>
	<!-- 来源单据号超链接 -->
	<bean id="alinkQueryHyperlinkMediator"
		class="nc.ui.pubapp.uif2app.linkquery.LinkQueryHyperlinkMediator">
		<property name="model" ref="ManageAppModel" />
		<property name="srcBillIdField" value="csrcid" />
		<!-- 超链字段名 -->
		<property name="srcBillNOField" value="vsrccode" />
		<!-- 保存源单据类型的字段名 -->
		<property name="srcBillTypeField" value="vsrctype" />
		<!-- 保存源单据类型的字段所在位置，0：表头；1：表体 -->
		<property name="srcBillTypeFieldPos" value="1" />
	</bean>
	<!-- 销售合同号超链接 -->
	<bean id="blinkQueryHyperlinkMediator"
		class="nc.ui.pubapp.uif2app.linkquery.LinkQueryHyperlinkMediator">
		<property name="model" ref="ManageAppModel" />
		<property name="srcBillIdField" value="cctmanageid" />
		<!-- 超链字段名 -->
		<property name="srcBillNOField" value="vctcode" />
		<!-- 保存源单据类型的字段名 -->
		<property name="srcBillTypeField" value="vsrctype" />
		<!-- 保存源单据类型的字段所在位置，0：表头；1：表体 -->
		<property name="srcBillTypeFieldPos" value="1" />
	</bean>
	<!--卡控件 -->
	<bean id="billFormEditor" class="nc.ui.so.m30.billui.view.SaleOrderBillForm"
		init-method="initUI">
		<property name="model" ref="ManageAppModel"></property>
		<property name="templateContainer" ref="templateContainer" />
		<!-- 是否进行模板非空校验 -->
		<property name="templateNotNullValidate" value="true" />
		<!-- 是否自动增行 -->
		<property name="autoAddLine" value="true" />
		<!-- 卡片自定义项处理 -->
		<property name="userdefitemPreparator">
			<bean class="nc.ui.pubapp.uif2app.view.CompositeBillDataPrepare">
				<property name="billDataPrepares">
					<list>
						<ref bean="userdefitemPreparator" />
						<ref bean="marAsstPreparator" />
					</list>
				</property>
			</bean>
		</property>
		<!-- 空行过滤器，卡片界面根据此过滤器删除表体空行 -->
		<property name="blankChildrenFilter">
			<!-- 简单的根据一个字段是否为空的空行过滤器，因为多数情况下都是判断存货是否为空来过滤空行 -->
			<bean
				class="nc.ui.pubapp.uif2app.view.value.MultiFieldsBlankChildrenFilter">
				<!-- 指定用来判断空行的字段名称，可能多数情况下是存货字段 -->
				<property name="filterMap">
					<map>
						<entry>
							<key>
								<value>bodytable1</value>
							</key><!--页签code -->
							<list>
								<value>cmaterialvid</value>
							</list><!--检验是否空行字段code -->
						</entry>
					</map>
				</property>
			</bean>
		</property>
		<!-- 卡片需要清除的超链接注册 -->
		<property name="clearHyperlink">
			<list>
				<value>vbillcode</value>
			</list>
		</property>
		<!-- 表体行操作按钮注册，与表体右键菜单保持一致 -->
		<property name="bodyLineActions">
			<list>
				<!-- 提供了一组默认实现 -->
				<ref bean="SaleOrderAddLineAction" />
				<ref bean="SaleOrderInsertLineAction" />
				<ref bean="SaleOrderDelLineAction" />
				<ref bean="SaleOrderCopyLineAction" />
				<ref bean="SaleOrderPasteLineAction" />
				<ref bean="SaleOrderPasteToTailAction" />
				<bean class=" nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
				<ref bean="RearrangeRowNoBodyLineAction" />
				<bean class=" nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
				<ref bean="DefaultBodyZoomAction" />
			</list>
		</property>
	</bean>
	<bean id="SaleOrderAddLineAction" class="nc.ui.pubapp.uif2app.actions.BodyAddLineAction" />
	<bean id="SaleOrderCopyLineAction" class="nc.ui.pubapp.uif2app.actions.BodyCopyLineAction" />
	<bean id="SaleOrderInsertLineAction" class="nc.ui.pubapp.uif2app.actions.BodyInsertLineAction" />
	<bean id="SaleOrderDelLineAction" class="nc.ui.so.m30.billui.action.line.SaleOrderDelLineAction" />
		
	<!--表体粘贴行 -->
	<bean id="SaleOrderPasteLineAction" class="nc.ui.so.m30.billui.action.line.SaleOrderPasteLineAction">
		<property name="clearItems">
			<list>
				<value>csaleorderbid</value>
				<value>ts</value>
				<value>nreqrsnum</value>
			</list>
		</property>
		<property name="editor" ref="billFormEditor" />
	</bean>
	<bean id="SaleOrderPasteToTailAction" class="nc.ui.so.m30.billui.action.line.SaleOrderPasteToTailAction">
		<property name="clearItems">
			<list>
				<value>csaleorderbid</value>
				<value>ts</value>
				<value>nreqrsnum</value>
			</list>
		</property>
	</bean>
		<!-- UE分隔符 -->
				<bean class=" nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
				<bean class="nc.ui.pubapp.uif2app.actions.RearrangeRowNoBodyLineAction" />
				<bean class=" nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
				<bean class="nc.ui.pubapp.uif2app.actions.DefaultBodyZoomAction">
					<property name="pos" value="1" />
				</bean>
	<bean id="RearrangeRowNoBodyLineAction"
		class="nc.ui.pubapp.uif2app.actions.RearrangeRowNoBodyLineAction" />
	<!-- 双击列表行后显示卡片 -->
	<bean id="DefaultBodyZoomAction" class="nc.ui.pubapp.uif2app.actions.DefaultBodyZoomAction">
		<property name="pos" value="1" />
	</bean>
	<bean id="mouseClickShowPanelMediator" class="nc.ui.pubapp.uif2app.view.MouseClickShowPanelMediator">
		<property name="listView" ref="listView" />
		<property name="showUpComponent" ref="billFormEditor" />
		<property name="hyperLinkColumn" value="vbillcode" />
	</bean>

	<!-- 切换到卡片界面的拦截器 -->
	<bean id="formInterceptor"
		class="nc.ui.pubapp.uif2app.actions.interceptor.ShowUpComponentInterceptor">
		<property name="showUpComponent" ref="billFormEditor" />
	</bean>

	<!-- 切换到列表界面的拦截器 -->
	<bean id="listInterceptor"
		class="nc.ui.pubapp.uif2app.actions.interceptor.ShowUpComponentInterceptor">
		<property name="showUpComponent" ref="listView" />
	</bean>
	
	<!-- 界面单据号控制类 -->
	<!-- 单据号规则如果是“支持断号补号”，则不管模板上设置的可编辑性，单据号都不允许修改 -->
	<bean id="billCodeMediator" class="nc.ui.scmpub.listener.BillCodeEditMediator"
		init-method="initUI">
		<!-- 模板上的单据号字段ItemKey -->
		<property name="billCodeKey" value="vbillcode" />
		<!-- 单据类型 -->
		<property name="billType" value="30" />
		<property name="billForm" ref="billFormEditor" />
	</bean>

	<!-- 关闭节点的提示 -->
	<bean id="ClosingListener" class="nc.ui.uif2.FunNodeClosingHandler">
		<property name="model" ref="ManageAppModel" />
		<property name="saveaction" ref="saveAction" />
		<property name="cancelaction" ref="cancelAction" />
	</bean>
	<bean id="InitDataListener"
		class="nc.ui.pubapp.uif2app.model.DefaultFuncNodeInitDataListener">
		<property name="context" ref="context" />
		<property name="model" ref="ManageAppModel" />
		<property name="voClassName" value="nc.vo.so.m30.entity.SaleOrderVO" />
		<property name="autoShowUpComponent" ref="billFormEditor" />
		<property name="queryAction" ref="queryAction" />
	</bean>
	<!-- 更改组织后同步设置界面上所有参照的组织值 <bean class="nc.ui.pubapp.uif2app.view.CardPanelOrgSetterForAllRefMediator"> 
		<constructor-arg ref="billFormEditor"/> <property name="model" ref="ManageAppModel"/> 
		</bean> -->

	<!-- 行号自动处理 -->
	<bean id="rowNoMediator" class="nc.ui.pubapp.uif2app.view.RowNoMediator">
		<property name="model" ref="ManageAppModel" />
		<property name="editor" ref="billFormEditor" />

	</bean>

	<!-- 卡片自定义项处理 -->
	<bean name="userdefitemPreparator" class="nc.ui.uif2.editor.UserdefitemContainerPreparator">
		<property name="container" ref="userdefitemContainer" />
		<property name="params">
			<list>
				<ref bean="userQueryParams1" />
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="so.so_saleorder" />
					<property name="pos" value="0" />
					<property name="prefix" value="vdef" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="so.so_saleorder_b" />
					<property name="pos" value="1" />
					<property name="prefix" value="vbdef" />
				</bean>
			</list>
		</property>
	</bean>
	<bean id="userQueryParams1" class="nc.ui.uif2.editor.UserdefQueryParam">
		<property name="mdfullname" value=" ic.OnhandDimVO" />
		<property name="pos" value="0" />
		<property name="prefix" value="vdef" />
	</bean>

	<!-- 列表自定义项处理 -->
	<bean name="userdefitemlistPreparator" class="nc.ui.uif2.editor.UserdefitemContainerListPreparator">
		<property name="container" ref="userdefitemContainer" />
		<property name="params">
			<list>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="so.so_saleorder" />
					<property name="pos" value="0" />
					<property name="prefix" value="vdef" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="so.so_saleorder_b" />
					<property name="pos" value="1" />
					<property name="tabcode" value="bodytable1" />
					<property name="prefix" value="vbdef" />
				</bean>
			</list>
		</property>
	</bean>
	<!-- 自由项组件的配置 -->
	<bean id="marAsstPreparator"
		class="nc.ui.pubapp.uif2app.view.material.assistant.MarAsstPreparator">
		<property name="model" ref="ManageAppModel" />
		<property name="container" ref="userdefitemContainer" />
		<property name="prefix" value="vfree" />
		<property name="materialField" value="cmaterialvid" />
		<!-- 以下四个为物料固定辅助属性，可以选择配置 -->
		<property name="storeStateField" value="cstateid" /> 
		<property name="projectField" value="cprojectid" /> 
		<property name="supplierField" value="cvendorid" /> 
		<property name="productorField" value="cproductorid" />
		<property name="signatureField" value="cmffileid" />
	</bean>
	<bean id="userdefitemContainer" class="nc.ui.uif2.userdefitem.UserDefItemContainer">
		<property name="context" ref="context" />
		<property name="params">
			<list>
				<ref bean="queryParams1" />
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="so.so_saleorder" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="so.so_saleorder_b" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="rulecode" value="materialassistant" />
				</bean>
			</list>
		</property>
	</bean>
	<bean id="queryParams1" class="nc.ui.uif2.userdefitem.QueryParam">
		<property name="mdfullname" value="ic.OnhandDimVO" />
	</bean>

	<!-- 自动修正卡片界面的分数显示控件(换算率) -->
	<bean id="fractionFixMediator" class="nc.ui.pubapp.uif2app.view.FractionFixMediator"
		init-method="initUI">
		<constructor-arg>
			<list>
				<ref bean="billFormEditor" />
			</list>
		</constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="listView" />
			</list>
		</constructor-arg>
	</bean>
	<!-- model与界面的表体排序同步组件(先支持页签关联元数据对象、非关联的不支持)我们也可以改PUB_BILLTEMPLET_T表 -->
	<bean id="billBodySortMediator" class="nc.ui.pubapp.uif2app.model.BillBodySortMediator">
		<constructor-arg ref="ManageAppModel" />
		<constructor-arg ref="billFormEditor" />
		<constructor-arg ref="listView" />
	</bean>
	<!-- 编辑事件 -->
	<bean id="appEventHandlerMediator" class="nc.ui.pubapp.uif2app.model.AppEventHandlerMediator">
		<property name="model" ref="ManageAppModel" />
		<property name="handlerMap">
			<map>
				<entry key="nc.ui.pubapp.uif2app.event.card.CardHeadTailBeforeEditEvent">
					<list>
						<bean class="nc.ui.so.m30.billui.editor.headevent.HeadBeforeEditHandler">
						<property name="billform" ref="billFormEditor" />
						</bean>
					</list>
				</entry>
				<entry key="nc.ui.pubapp.uif2app.event.card.CardHeadTailAfterEditEvent">
					<list>
						<bean class="nc.ui.so.m30.billui.editor.headevent.HeadAfterEditHandler">
							<property name="billform" ref="billFormEditor" />
						</bean>
					</list>
				</entry>
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyBeforeEditEvent">
					<list>
						<bean class="nc.ui.so.m30.billui.editor.bodyevent.BodyBeforeEditHandler">
							<property name="billform" ref="billFormEditor" />
						</bean>
					</list>
				</entry>
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyAfterEditEvent">
					<list>
						<bean class="nc.ui.so.m30.billui.editor.bodyevent.BodyAfterEditHandler">
							<property name="billform" ref="billFormEditor" />
						</bean>
					</list>
				</entry>
				<!-- 表体行操作（增行、插入行、删行等） -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyAfterRowEditEvent">
					<list>
						<bean
							class="nc.ui.so.m30.billui.editor.bodyevent.CardBodyAfterRowEditHandler" />
					</list>
				</entry>
				<!-- 表体行操作事件（增行、插入行、删行等） -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyRowEditEvent">
					<list>
						<bean
							class="nc.ui.so.m30.billui.editor.bodyevent.CardBodyRowEditHandler" />
					</list>
				</entry>
				<entry key="nc.ui.pubapp.uif2app.event.OrgChangedEvent">
					<list>
						<bean class="nc.ui.so.m30.billui.editor.orgevent.OrgEditHandler">
							<constructor-arg ref="billFormEditor" />
							<constructor-arg ref="context" />
						</bean>
					</list>
				</entry>

			</map>
		</property>
	</bean>

	<!-- 界面布局总装########################################################### -->
	<bean id="container" class="nc.ui.uif2.TangramContainer"
		init-method="initUI">
		<property name="model" ref="ManageAppModel" />
		<property name="tangramLayoutRoot">
			<bean class="nc.ui.uif2.tangramlayout.node.TBNode">
				<property name="showMode" value="CardLayout"></property>
				<property name="tabs">
					<list>
						<bean class="nc.ui.uif2.tangramlayout.node.HSNode">
							<property name="left">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="component" ref="queryArea" />
								</bean>
							</property>
							<property name="right">
								<bean class="nc.ui.uif2.tangramlayout.node.VSNode">
									<property name="up">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<property name="component" ref="listToolbarPnl" />
										</bean>
									</property>
									<property name="down">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<!-- <property name="name" value="列表" /> -->
											<property name="name">
												<bean class="nc.ui.uif2.I18nFB">
													<property name="resDir" value="common" />
													<property name="resId" value="UC001-0000107" />
													<property name="defaultValue" value="列表" />
												</bean>
											</property>
											<property name="component" ref="listView" />
										</bean>
									</property>
									<property name="dividerLocation" value="25f" />
								</bean>
							</property>
							<property name="dividerLocation" value="0.22f" />
						</bean>
						<bean class="nc.ui.uif2.tangramlayout.node.HSNode">
							<property name="left">
								<bean class="nc.ui.uif2.tangramlayout.node.VSNode">
									<property name="up">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<property name="component" ref="cardToolbarPnl" />
										</bean>
									</property>
									<property name="down">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<property name="component" ref="billFormEditor" />
											<!-- <property name="name" value="卡片" /> -->
											<property name="name">
												<bean class="nc.ui.uif2.I18nFB">
													<property name="resDir" value="common" />
													<property name="resId" value="UC001-0000106" />
													<property name="defaultValue" value="卡片" />
												</bean>
											</property>
										</bean>
									</property>
									<property name="dividerLocation" value="30f" />
								</bean>
							</property>
							<property name="right">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="component" ref="besidewidget" />
								</bean>
							</property>
							<property name="dividerLocation" value="0.8f" />
						</bean>
					</list>
				</property>
			</bean>
		</property>
	</bean>
	<!-- 查询方案 -->
	<bean id="queryArea" class="nc.ui.pubapp.uif2app.tangramlayout.UEQueryAreaShell"
		init-method="initUI">
		<property name="queryAreaCreator" ref="queryAction" />
	</bean>
	<!-- 列表顶端工具栏面板 -->
	<bean id="listToolbarPnl" class="nc.ui.uif2.tangramlayout.CardLayoutToolbarPanel">
		<property name="model" ref="ManageAppModel" />
